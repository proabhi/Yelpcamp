<%- include partials/header.ejs %>



<div class="container">
    <form action="/create" autocomplete="off" method="POST">
        <div class="form-group">
            <label for="exampleInputEmail1">Campground Name</label>
            <input type="text" name="name" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                placeholder="Enter campground name">
            <small id="emailHelp" class="form-text text-muted">Keep it short and sweet...</small>
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Image</label>
            <input type="text" name="image" class="form-control" id="exampleInputPassword1" placeholder="Image source">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>



<%- include partials/footer.ejs %>